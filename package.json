{
  "name": "react-loading-shell",
  "version": "1.0.0",
  "description": "A React shell for CSS loading animations.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "lint:fix": "prettier-eslint '**/*.js' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanadium-vole/react-loading-shell.git"
  },
  "keywords": [
    "react",
    "css",
    "spinners"
  ],
  "author": "Danesh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vanadium-vole/react-loading-shell/issues"
  },
  "homepage": "https://github.com/vanadium-vole/react-loading-shell#readme",
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier-eslint": "^9.0.0",
    "prettier-eslint-cli": "^5.0.0"
  }
}
